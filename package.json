{
    "name": "rdfox-rdf",
    "displayName": "RDFox Syntax Highlighting",
    "description": "SPARQL, Turtle, TriG, N-Triples, N-Quads, Datalog rules and RDFox shell script syntax highlighting",
    "version": "1.5.1",
    "publisher": "rdfox",
    "repository": {
        "type": "git",
        "url": "https://github.com/OxfordSemantic/vscode-rdfox-rdf.git"
    },
    "engines": {
        "vscode": "^1.62.0"
    },
    "scripts": {
        "vscode:prepublish": "tsc"
    },
    "license": "MIT",
    "categories": [
        "Programming Languages"
    ],
    "keywords": [
        "RDF",
        "Graph",
        "Graph Database",
        "Query",
        "OWL"
    ],
    "extensionKind": [
        "ui",
        "workspace"
    ],
    "icon": "images/icon.png",
    "main": "./out/extension.js",
    "activationEvents": [
        "onCommand:rdfox-rdf.profile-query",
        "onCommand:rdfox-rdf.profile-reasoning",
        "onLanguage:rdfox",
        "onLanguage:sparql",
        "onLanguage:datalog",
        "onLanguage:turtle",
        "onLanguage:trig",
        "onLanguage:n-triples",
        "onLanguage:n-quads",
        "onCommand:rdfox-rdf.open-in-console",
        "onCommand:rdfox-rdf.upload-rule",
        "onCommand:rdfox-rdf.upload-rule-from-selection",
        "onCommand:rdfox-rdf.upload-rule-from-explorer",
        "onCommand:rdfox-rdf.delete-rule",
        "onCommand:rdfox-rdf.delete-rule-from-selection",
        "onCommand:rdfox-rdf.delete-rule-from-explorer",
        "onCommand:rdfox-rdf.upload-data",
        "onCommand:rdfox-rdf.upload-data-from-selection",
        "onCommand:rdfox-rdf.upload-data-from-explorer",
        "onCommand:rdfox-rdf.delete-data",
        "onCommand:rdfox-rdf.delete-data-from-selection",
        "onCommand:rdfox-rdf.delete-data-from-explorer",
        "onCommand:rdfox-rdf.open-settings"
    ],
    "contributes": {
        "languages": [
            {
                "id": "rdfox",
                "aliases": [
                    "RDFox Shell Script",
                    "RDFox"
                ],
                "extensions": [
                    ".rdfox"
                ],
                "configuration": "./configuration/rdfox-language-configuration.json",
                "icon": {
                    "light": "./icons/rdfox-offset.svg",
                    "dark": "./icons/rdfox-offset.svg"
                }
            },
            {
                "id": "datalog",
                "aliases": [
                    "Datalog",
                    "dlog"
                ],
                "extensions": [
                    ".dlog",
                    ".datalog"
                ],
                "configuration": "./configuration/datalog-language-configuration.json",
                "icon": {
                    "light": "./icons/datalog-offset.svg",
                    "dark": "./icons/datalog-offset.svg"
                }
            },
            {
                "id": "sparql",
                "aliases": [
                    "SPARQL"
                ],
                "extensions": [
                    ".rq",
                    ".sparql"
                ],
                "configuration": "./configuration/turtle-language-configuration.json",
                "icon": {
                    "light": "./icons/sparql-light-offset.svg",
                    "dark": "./icons/sparql-dark-offset.svg"
                }
            },
            {
                "id": "n-triples",
                "aliases": [
                    "N-Triples",
                    "nt"
                ],
                "extensions": [
                    ".nt"
                ],
                "configuration": "./configuration/turtle-language-configuration.json",
                "icon": {
                    "light": "./icons/semantic-web-cube-offset.svg",
                    "dark": "./icons/semantic-web-cube-offset.svg"
                }
            },
            {
                "id": "n-quads",
                "aliases": [
                    "N-Quads",
                    "nq"
                ],
                "extensions": [
                    ".nq"
                ],
                "configuration": "./configuration/turtle-language-configuration.json",
                "icon": {
                    "light": "./icons/semantic-web-cube-offset.svg",
                    "dark": "./icons/semantic-web-cube-offset.svg"
                }
            },
            {
                "id": "turtle",
                "aliases": [
                    "Turtle",
                    "ttl"
                ],
                "extensions": [
                    ".ttl"
                ],
                "configuration": "./configuration/turtle-language-configuration.json",
                "icon": {
                    "light": "./icons/semantic-web-cube-offset.svg",
                    "dark": "./icons/semantic-web-cube-offset.svg"
                }
            },
            {
                "id": "trig",
                "aliases": [
                    "TriG"
                ],
                "extensions": [
                    ".trig"
                ],
                "configuration": "./configuration/turtle-language-configuration.json",
                "icon": {
                    "light": "./icons/semantic-web-cube-offset.svg",
                    "dark": "./icons/semantic-web-cube-offset.svg"
                }
            }
        ],
        "grammars": [
            {
                "language": "rdfox",
                "scopeName": "source.rdfox",
                "path": "./syntaxes/rdfox.tmLanguage.json"
            },
            {
                "language": "datalog",
                "scopeName": "source.datalog",
                "path": "./syntaxes/datalog.tmLanguage.json"
            },
            {
                "language": "sparql",
                "scopeName": "source.sparql",
                "path": "./syntaxes/sparql.tmLanguage.json"
            },
            {
                "language": "n-triples",
                "scopeName": "source.n-triples",
                "path": "./syntaxes/n-triples.tmLanguage.json"
            },
            {
                "language": "n-quads",
                "scopeName": "source.n-quads",
                "path": "./syntaxes/n-quads.tmLanguage.json"
            },
            {
                "language": "turtle",
                "scopeName": "source.turtle",
                "path": "./syntaxes/turtle.tmLanguage.json"
            },
            {
                "language": "trig",
                "scopeName": "source.trig",
                "path": "./syntaxes/trig.tmLanguage.json"
            }
        ],
        "commands": [
            {
                "command": "rdfox-rdf.open-in-console",
                "title": "Open in RDFox Web Console",
                "shortTitle": "Open in Console",
                "icon": "./icons/open-in-console.svg"
            },
            {
                "command": "rdfox-rdf.upload-rule",
                "title": "Upload Datalog rules to RDFox",
                "shortTitle": "Upload rules",
                "icon": "./icons/upload-rule.svg"
            },
            {
                "command": "rdfox-rdf.upload-rule-from-selection",
                "title": "Upload selected Datalog rules to RDFox",
                "shortTitle": "Upload selected rules",
                "icon": "./icons/upload-rule.svg"
            },
            {
                "command": "rdfox-rdf.upload-rule-from-explorer",
                "title": "Upload Datalog rules to RDFox",
                "shortTitle": "Upload rules",
                "icon": "./icons/upload-rule.svg"
            },
            {
                "command": "rdfox-rdf.delete-rule",
                "title": "Delete Datalog rules from RDFox",
                "shortTitle": "Delete rules",
                "icon": "./icons/delete-rule.svg"
            },
            {
                "command": "rdfox-rdf.delete-rule-from-selection",
                "title": "Delete selected Datalog rules from RDFox",
                "shortTitle": "Delete selected rules",
                "icon": "./icons/delete-rule.svg"
            },
            {
                "command": "rdfox-rdf.delete-rule-from-explorer",
                "title": "Delete Datalog rules from RDFox",
                "shortTitle": "Delete rules",
                "icon": "./icons/delete-rule.svg"
            },
            {
                "command": "rdfox-rdf.upload-data",
                "title": "Upload data to RDFox",
                "shortTitle": "Upload data",
                "icon": "./icons/upload-rule.svg"
            },
            {
                "command": "rdfox-rdf.upload-data-from-selection",
                "title": "Upload selected data to RDFox",
                "shortTitle": "Upload selected data",
                "icon": "./icons/upload-rule.svg"
            },
            {
                "command": "rdfox-rdf.upload-data-from-explorer",
                "title": "Upload data to RDFox",
                "shortTitle": "Upload data",
                "icon": "./icons/upload-rule.svg"
            },
            {
                "command": "rdfox-rdf.delete-data",
                "title": "Delete data from RDFox",
                "shortTitle": "Delete data",
                "icon": "./icons/delete-rule.svg"
            },
            {
                "command": "rdfox-rdf.delete-data-from-selection",
                "title": "Delete selected data from RDFox",
                "shortTitle": "Delete selected data",
                "icon": "./icons/delete-rule.svg"
            },
            {
                "command": "rdfox-rdf.delete-data-from-explorer",
                "title": "Delete data from RDFox",
                "shortTitle": "Delete data",
                "icon": "./icons/delete-rule.svg"
            },
            {
                "command": "rdfox-rdf.profile-query",
                "title": "Run query profiling",
                "shortTitle": "Profile Query"
            },
            {
                "command": "rdfox-rdf.profile-reasoning",
                "title": "Run reasoning profiling",
                "shortTitle": "Profile Reasoning"
            },
            {
                "command": "rdfox-rdf.open-settings",
                "title": "Open RDFox extension settings",
                "icon": "./icons/extension-settings.svg"
            }
        ],
        "configuration": {
            "title": "RDFox Syntax Highlighting",
            "properties": {
                "RDFox.URL": {
                    "type": "string",
                    "default": "http://localhost:12110",
                    "description": "URL of the RDFox endpoint"
                },
                "RDFox.datastoreName": {
                    "type": "string",
                    "default": "default",
                    "description": "Name of the RDFox datastore"
                }
            }
        },
        "configurationDefaults": {
            "[rdfox]": {
                "editor.wordBasedSuggestions": "allDocuments"
            },
            "[datalog]": {
                "editor.wordBasedSuggestions": "allDocuments"
            },
            "[sparql]": {
                "editor.wordBasedSuggestions": "allDocuments"
            },
            "[turtle]": {
                "editor.wordBasedSuggestions": "allDocuments"
            },
            "[trig]": {
                "editor.wordBasedSuggestions": "allDocuments"
            },
            "[n-triples]": {
                "editor.wordBasedSuggestions": "allDocuments"
            },
            "[n-quads]": {
                "editor.wordBasedSuggestions": "allDocuments"
            }
        },
        "menus": {
            "editor/title": [
                {
                    "command": "rdfox-rdf.open-in-console",
                    "group": "navigation@1",
                    "when": "editorLangId == sparql"
                },
                {
                    "command": "rdfox-rdf.upload-rule",
                    "group": "navigation@1",
                    "when": "editorLangId == datalog"
                },
                {
                    "command": "rdfox-rdf.delete-rule",
                    "group": "navigation@2",
                    "when": "editorLangId == datalog"
                },
                {
                    "command": "rdfox-rdf.upload-data",
                    "group": "navigation@1",
                    "when": "editorLangId == n-triples || editorLangId == n-quads || editorLangId == turtle || editorLangId == trig"
                },
                {
                    "command": "rdfox-rdf.delete-data",
                    "group": "navigation@2",
                    "when": "editorLangId == n-triples || editorLangId == n-quads || editorLangId == turtle || editorLangId == trig"
                },
                {
                    "command": "rdfox-rdf.open-settings",
                    "group": "navigation@3",
                    "when": "editorLangId == datalog || editorLangId == sparql || editorLangId == n-triples || editorLangId == n-quads || editorLangId == turtle || editorLangId == trig"
                }
            ],
            "editor/context": [
                {
                    "command": "rdfox-rdf.upload-rule-from-selection",
                    "group": "0_actions",
                    "when": "editorLangId == datalog"
                },
                {
                    "command": "rdfox-rdf.delete-rule-from-selection",
                    "group": "0_actions",
                    "when": "editorLangId == datalog"
                },
                {
                    "command": "rdfox-rdf.upload-data-from-selection",
                    "group": "0_actions",
                    "when": "editorLangId == n-triples || editorLangId == n-quads || editorLangId == turtle || editorLangId == trig"
                },
                {
                    "command": "rdfox-rdf.delete-data-from-selection",
                    "group": "0_actions",
                    "when": "editorLangId == n-triples || editorLangId == n-quads || editorLangId == turtle || editorLangId == trig"
                }
            ],
            "explorer/context": [
                {
                    "command": "rdfox-rdf.upload-rule-from-explorer",
                    "group": "1_rdfox@1",
                    "when": "resourceLangId == datalog"
                },
                {
                    "command": "rdfox-rdf.upload-data-from-explorer",
                    "group": "1_rdfox@2",
                    "when": "resourceLangId == n-triples || resourceLangId == n-quads || resourceLangId == turtle || resourceLangId == trig"
                },
                {
                    "command": "rdfox-rdf.delete-rule-from-explorer",
                    "group": "1_rdfox@3",
                    "when": "resourceLangId == datalog"
                },
                {
                    "command": "rdfox-rdf.delete-data-from-explorer",
                    "group": "1_rdfox@4",
                    "when": "resourceLangId == n-triples || resourceLangId == n-quads || resourceLangId == turtle || resourceLangId == trig"
                },
                {
                    "command": "rdfox-rdf.profile-query",
                    "group": "1_rdfox@5",
                    "when": "resourceLangId == sparql"
                },
                {
                    "command": "rdfox-rdf.profile-reasoning",
                    "group": "1_rdfox@6",
                    "when": "resourceLangId == datalog"
                }
            ]
        }
    },
    "devDependencies": {
        "@types/node": "^20.4.0",
        "@types/node-fetch": "^2.6.4",
        "@types/uuid": "^9.0.2",
        "@types/vscode": "^1.62.0",
        "typescript": "^5.1.6"
    },
    "dependencies": {
        "uuid": "^9.0.0"
    }
}
